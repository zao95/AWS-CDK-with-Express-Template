{
    "name": "cdk-awmaker",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "npm run build:infra && npm run build:server",
        "build:server": "babel ./src --out-dir ./dist --extensions \".ts\" && npm install --only=prod --prefix ./dist",
        "build:infra": "babel ./infra --out-dir ./infra-dist --extensions \".ts\"",
        "express-offline": "npm run build:server && set NODE_ENV=Offline &&node ./dist/entry.js",
        "bootstrap": "npm run build && cdk bootstrap --profile educate",
        "deploy": "npm run build && cdk deploy --profile educate",
        "destroy": "cdk destroy --profile educate"
    },
    "author": "Lee Jeong-Woo",
    "license": "ISC",
    "devDependencies": {
        "@aws-cdk/aws-apigateway": "^1.91.0",
        "@aws-cdk/aws-lambda": "^1.91.0",
        "@aws-cdk/core": "^1.91.0",
        "@babel/cli": "^7.13.0",
        "@babel/core": "^7.13.8",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
        "@babel/preset-env": "^7.13.8",
        "@babel/preset-typescript": "^7.13.0",
        "@types/aws-lambda": "^8.10.72",
        "@types/node": "^14.14.31",
        "@types/aws-serverless-express": "^3.3.3",
        "aws-cdk": "^1.91.0",
        "core-js": "^3.9.1",
        "typescript": "^4.2.2"
    },
    "dependencies": {
        "aws-serverless-express": "^3.4.0",
        "express": "^4.17.1"
    }
}
